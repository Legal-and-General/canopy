{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lg",
      "camelCase"
    ],
    "component-selector": [
      true,
      ["element", "attribute"],
      "lg",
      "kebab-case"
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
