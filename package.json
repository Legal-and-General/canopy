{
  "name": "@legal-and-general/canopy",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "url": "git://github.com/Legal-and-General/canopy.git",
  "repository": "https://github.com/Legal-and-General/canopy",
  "scripts": {
    "commit": "npx git-cz",
    "ng": "ng",
    "start": "npm run storybook",
    "start:test-app": "ng serve",
    "build": "ng build && npm run build-styles",
    "build:test-app": "npm run build && ng build canopy-test-app --prod --outputPath ./test-dist",
    "build:icons": "npm run optimise:svgs && npm run generate:icons && npm run generate:brand-icons",
    "generate:icons": "svg-to-ts-constants --config svg-to-ts-icons.config.js",
    "generate:brand-icons": "svg-to-ts-constants --config svg-to-ts-brand-icons.config.js",
    "optimise:svgs": "svgo projects/canopy/src/assets/icons -r --config=svgo-icons.config.js && svgo projects/canopy/src/assets/brand-icons -r --config=svgo-brand-icons.config.js",
    "test": "ng test",
    "test:test-app": "ng test canopy-test-app",
    "test:ci": "ng test canopy --watch=false --browsers=ChromeHeadless && ng test canopy-test-app --watch=false --browsers=ChromeHeadless",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "ng lint",
    "e2e": "ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json --disablePrivate -e json -d .",
    "storybook": "npm run docs:json && start-storybook -s ./assets",
    "stylelint": "stylelint \"projects/**/*.scss\"",
    "build-storybook": "build-storybook --quiet -s ./assets",
    "build-styles": "webpack --config webpack.global.config.js",
    "postinstall": "ngcc",
    "license-check:build": "npx license-checker --summary --start dist/canopy --onlyAllow 'MIT;Apache-2.0;0BSD'",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "core-js": "^3.22.3",
    "date-fns": "^2.28.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.4.0",
    "webpack": "^4.46.0",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.4",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "^11.2.19",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-angular": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt": "^16.2.3",
    "@compodoc/compodoc": "^1.1.19",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addons": "^6.4.22",
    "@storybook/angular": "^6.4.22",
    "@storybook/theming": "^6.4.22",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@stylelint/postcss-markdown": "^0.36.1",
    "@types/jasmine": "^4.0.3",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^14.18.15",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "autoprefixer": "^10.4.5",
    "babel-loader": "^8.2.5",
    "codelyzer": "^6.0.2",
    "color-convert": "^2.0.1",
    "commitizen": "^4.2.4",
    "copy-webpack-plugin": "^6.4.1",
    "css-vars-ponyfill": "^2.4.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-unused-imports": "^1.1.5",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jasmine-core": "^4.1.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^12.4.1",
    "mini-css-extract-plugin": "^1.6.2",
    "ng-mocks": "^13.4.2",
    "ng-packagr": "^11.2.4",
    "postcss": "^8.4.12",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.0",
    "protractor": "^7.0.0",
    "sass": "^1.51.0",
    "semantic-release": "^19.0.2",
    "stylelint": "^14.8.1",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "svg-to-ts": "^8.6.1",
    "svgo": "^2.7.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.7.0",
    "tsickle": "^0.46.0",
    "typescript": "4.1.6",
    "webpack-cli": "^4.9.2",
    "webpack-fix-style-only-entries": "^0.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
